package com.ssw.int1;

public class PrePostIncrement1 {
	public static void main(String[] args) {
		
	}
	
	public static void methodOne() {
		int i=5;
		System.out.println(++i);
		System.out.println(i);
		System.out.println(i++);
	}
	
	public static void methodOne() {
		int i=5;
		System.out.println(++i);
		System.out.println(i);
		System.out.println(i++);
	}

}
